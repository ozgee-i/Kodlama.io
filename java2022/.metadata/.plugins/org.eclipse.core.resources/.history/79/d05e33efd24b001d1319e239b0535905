package classesWithAttributes;

public class Product {
	//attibbute | field
	private int id;
	private String name;
	private String description;
	private double price;
	private int stockAmount;
	private String renk;
	private String kod;
	
	//getter ve setter
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id=id;
	}

}
